PLACE
INTERNAL
  p0,  { initial }
  pa1,
  pa2,
  pa3,
  pa4,
  pa5,
  pb1,
  pb2,
  pb3,
  pb4,
  pb5,
  pc1,
  pc2,
  pc3,
  pc4,
  pc5,
  pd1,
  pd2,
  pd3,
  pd4,
  pd5;

OUTPUT
