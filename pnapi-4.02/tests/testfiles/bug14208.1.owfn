PLACE
 INTERNAL
  p0,p1,p2,p3;
 INPUT
  MEuro;
 OUTPUT
  MBevT,MBevC;
 SYNCHRONOUS
  MTB,MCB;

INITIALMARKING
  p0;

FINALMARKING
  p0;

TRANSITION t0 { ?MEuro }
 CONSUME
  p0, MEuro;
 PRODUCE
  p1;

TRANSITION t1 { #MTB }
 CONSUME
  p1;
 PRODUCE
  p2;
 SYNCHRONIZE
  MTB;

TRANSITION t2 { #MCB }
 CONSUME
  p1;
 PRODUCE
  p3;
 SYNCHRONIZE
  MCB;

TRANSITION t3 { ?MBevT }
 CONSUME
  p2;
 PRODUCE
  p0, MBevT;

TRANSITION t4 { ?MBevC }
 CONSUME
  p3;
 PRODUCE
  p0, MBevC;

