PLACE
  INTERNAL
    p0, p1, p2, p3, p4;

  INPUT
    E;

  OUTPUT
    B;

  SYNCHRONOUS
    C, T;

INITIALMARKING
  p0;

FINALCONDITION
  ((p0 = 1)) AND ALL_OTHER_PLACES_EMPTY;


TRANSITION t1
  CONSUME E, p0;
  PRODUCE p1;

TRANSITION t2
  CONSUME p1;
  PRODUCE p2;
  SYNCHRONIZE C;

TRANSITION t3
  CONSUME p1;
  PRODUCE p3;
  SYNCHRONIZE T;

TRANSITION t4
  CONSUME p2;
  PRODUCE B, p4;

TRANSITION t5
  CONSUME p3;
  PRODUCE B, p4;

TRANSITION t6
  CONSUME p4;
  PRODUCE p0;
