{
Note: In this net input labels contain an o and output labels an i.
}

INTERFACE
  PORT remain
    INPUT
      rbi1, rbi2;
    OUTPUT
      rbo1, rbo2;
    SYNCHRONOUS
      rbs1, rbs2;

  PORT common_1
    INPUT
      co1;
    OUTPUT
      ci2;
    SYNCHRONOUS
      cs1;

  PORT common_2
    INPUT
      co2;
    OUTPUT
      ci1;
    SYNCHRONOUS
      cs2;

PLACE
  p0, p1, p2, p3, p4, p5, p6, p7;

INITIALMARKING
  p0;

FINALCONDITION
  (p6 = 1) AND ALL_OTHER_PLACES_EMPTY;

TRANSITION t1
  CONSUME p0;
  PRODUCE p1, rbo1;

TRANSITION t2
  CONSUME p1, rbi1;
  PRODUCE p2;

TRANSITION t3
  CONSUME p2;
  PRODUCE p3;
  SYNCHRONIZE rbs1;

TRANSITION t4
  CONSUME p3;
  PRODUCE p4, ci1;

TRANSITION t5
  CONSUME p4, co1;
  PRODUCE p5;

TRANSITION t6
  CONSUME p5;
  PRODUCE p6;
  SYNCHRONIZE cs1;

TRANSITION t7
  CONSUME p5;
  PRODUCE p6;
  SYNCHRONIZE cs1;

TRANSITION t8
  CONSUME;
  PRODUCE;

